    "morgan": "^1.7.0",
    "pretty-error": "^2.0.0",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.2.1",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "vega": "^2.6.0",
    "webpack": "^1.13.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "ignores": [],
    "envs": [
      "browser"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never"
        }
      ],
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  }
}
